{
  "name": "lit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run npminstall && npm run dev\"",
    "npminstall": "npm install --prefix .. && npm install",
    "dev": "concurrently \"npm:bedev\" \"npm:fedev\"",
    "bedev": "node ../bff.cjs",
    "fedev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lit": "^3.1.2"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
